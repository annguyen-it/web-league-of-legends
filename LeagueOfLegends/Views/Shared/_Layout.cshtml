<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>@ViewBag.Title - League of Legends</title>
  <script src="https://kit.fontawesome.com/9ed593a796.js" crossorigin="anonymous"></script>
  <link href="@Url.Content("~/images/favicon/favicon.ico")" rel="shortcut icon" type="image/x-icon" />
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
  @Styles.Render("~/Content/css")
  @Styles.Render("~/Content/shared")
  @if (IsSectionDefined("AddToHeader"))
  {
    @RenderSection("AddToHeader", required: false);
  }
</head>
<body>
  <nav class="navbar navbar-expand-xl fixed-top">
    @Html.Partial("_NavBar")
  </nav>

  @RenderBody()

  <div class="footer">
    @Html.Partial("_Footer")
  </div>

  <script src="https://code.jquery.com/jquery-3.6.0.min.js" integrity="sha256-/xUj+3OJU5yExlq6GSYGSHk7tPXikynS7ogEvDej/m4=" crossorigin="anonymous"></script>
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
  <script src="https://unpkg.com/imagesloaded@5/imagesloaded.pkgd.min.js"></script>

  <script src="@Url.Content("~/Scripts/Site/CanvasRect.js")"></script>
  <script src="@Url.Content("~/Scripts/Site/Svg.js")"></script>
  <script src="@Url.Content("~/Scripts/Site/Shared/NavBar.js")"></script>
  
  @if (IsSectionDefined("Scripts"))
  {
    @RenderSection("Scripts", required: false);
  }
</body>
</html>
